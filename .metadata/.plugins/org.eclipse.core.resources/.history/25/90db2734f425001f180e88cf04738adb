
public class Conta_Corrente extends Conta {
	
	public double deposito;
	public double saque;
	public double transferencia;
	public double saldo;
	
	
	public void Deposito(double deposito) {
		this.deposito = deposito;
		System.out.printf("Deposito de %.2f realizado ", deposito);
		System.out.println();
		
	}
	
	public void Saque(double saque) {
		this.saque = saque;
		System.out.printf("Saque de %.2f realizado ", saque);
		System.out.println();
	}
	
	public void Transferencia(double transferencia) {
		this.transferencia = transferencia;
		System.out.printf("Transferencia de %.2f realizada ", transferencia);
		System.out.println();
	}
	
	public void Saldo() {
		this.saldo = deposito - saque - transferencia;
		System.out.printf("Seu saldo Ã© %.2f ", saldo);
	}

	@Override
	public void Deposito() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void Saque() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void Transferencia() {
		// TODO Auto-generated method stub
		
	}
}
